{
    "name": "Image Changer",
    "description": "Extension to change image on google.com",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "tabs"],
    "host_permissions": ["https://*.youtube.com/*"],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://*.youtube.com/*"],
            "js": ["contentScript.js"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "asset/bookmark.png",
                "asset/play.png",
                "asset/delete.png",
                "assets/save.png"
            ],
            "matches": ["https://*.youtube.com/*"]
        }
    ]


}